{
  "name": "@car-cutter/vue-webplayer",
  "version": "0.13.0",
  "license": "Apache-2.0",
  "author": {
    "name": "CarCutter",
    "url": "https://www.car-cutter.com/"
  },
  "homepage": "https://carcutter.github.io/cars-webplayer-js/",
  "description": "Use CarCutter WebPlayer in your Vue app",
  "keywords": [
    "car-cutter",
    "webplayer",
    "vue"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vue2": {
      "types": "./dist/vue2.d.ts",
      "import": "./dist/vue2.js",
      "require": "./dist/vue2.cjs"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "lint": "eslint . --ext ts --max-warnings 0",
    "analyze": "vite-bundle-visualizer"
  },
  "dependencies": {
    "@car-cutter/wc-webplayer": "0.13.0"
  },
  "peerDependencies": {
    "vue": ">=2"
  },
  "devDependencies": {
    "@car-cutter/eslint-config": "*",
    "@car-cutter/typescript-config": "*",
    "@vitejs/plugin-vue": "^5.1.3",
    "browserslist": "^4.23.3",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.18",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "^4.2.1",
    "vue": "^3.5.6",
    "vue-tsc": "^2.1.6"
  },
  "browserslist": [
    "defaults"
  ]
}
